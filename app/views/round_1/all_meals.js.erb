document.addEventListener('DOMContentLoaded', function () {
  var chart = Highcharts.chart({
    chart: {
      renderTo: 'all_meals',
      type: 'line'
    },
    series: [{
      name: 'Meals served',
      data: [
<%= @data %>
      ]
    }],
    title: {
      text: 'All recorded meals'
    },
    xAxis: {
      title: {
        text: 'Meal date'
      },
      type: 'datetime',
    },
    yAxis: {
      title: {
        text: 'Number of plates served'
      }
    }
  });
});
